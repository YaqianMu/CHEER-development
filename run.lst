 446  *----------------------------------------------*
 447  *==calibrate benchmark quantities
 448  *----------------------------------------------*
 449   
 450  int0(i,j)   = sam(i,j);
 451  fact0(f,j)  = sam(f,j);
 452  ffact0(x)   = theta(x)*sam("capital",x);
 453  fact0("capital",x) =(1-theta(x))*sam("capital",x);
 454   
 455  display ffact0,fact0,int0;
 456   
 457  inv0(i)     =max(0,sam(i,"investment"));
 458  invf0(f)    =max(0,sam(f,"investment"));
 459   
 460  xinv0(i)     =min(0,sam(i,"investment"));
 461  xinvf0(f)    =min(0,sam(f,"investment"));
 462   
 463  cons0(i)    =max(0,sam(i,"Household"))+max(0,sam(i,"GOVERNMENT"));
 464  consf0(f)   =max(0,sam(f,"Household"))+max(0,sam(f,"GOVERNMENT"));
 465   
 466  xcons0(i)    =min(0,sam(i,"Household"))+min(0,sam(i,"GOVERNMENT"));
 467  xconsf0(f)   =min(0,sam(f,"Household"))+min(0,sam(f,"GOVERNMENT"));
 468   
 469  exp0(i)    =max(0,sam(i,"export"));
 470  expf0(f)   =max(0,sam(f,"export"));
 471   
 472  xexp0(i)    =min(0,sam(i,"export"));
 473  xexpf0(f)   =min(0,sam(f,"export"));
 474   
 475  imp0(i)    =max(0,-sam(i,"import"));
 476  impf0(f)   =max(0,-sam(f,"import"));
 477   
 478  ximp0(i)    =min(0,-sam(i,"import"));
 479  ximpf0(f)   =min(0,-sam(f,"import"));
 480   
 481  nx0(i)      =exp0(i)-imp0(i)-(xexp0(i)-ximp0(i));
 482  nxf0(f)     =expf0(f)-impf0(f)-(xexpf0(f)-ximpf0(f));
 483   
 484  output0(i)  =sum(j,sam(i,j))+inv0(i)+cons0(i)+nx0(i)+(xinv0(i)+xcons0(i));
 485   
 486  fact(f)     =sum(j,fact0(f,j))+invf0(f)+consf0(f)+nxf0(f)+(xinvf0(f)+xcons
      f0(f));
